void loop() {
  // Connecting to Adafruit IO Platform
  MQTT_connect();
  
  //Obtaining Sensor Data
  //Reading Humidity and Temperature Data - Sensor 1
  float humidity = dht.readHumidity();
  float temperature = dht.readTemperature();
  if (isnan(humidity) || isnan(temperature)) {
    Serial.println(F("Failed to read from DHT sensor!"));
    humidity = 0;
    temperature = 0;
  }

  //Reading Air Quality Data - Sensor 2
  int airquality = analogRead(MQ135);

  //Reading Vibration Data - Sensor 3
  vibrationfinal = 0;
  for (int i = 0; i <= 1023; i++) {
    vibration = digitalRead(SW420);
    vibrationfinal += vibration;
  }
  vibrationfinal = map(vibrationfinal, 0, 1023, 0, 100);

  //Reading Air Pressure Data - Sensor 4
  float pressure = (scale.read());
  pressure = pressure - 2450000;      // add offset
  pressure = pressure * (-0.000018);  // add scale factor


  //Printing in Serial Moniter
  Serial.print(F("Humidity: "));
  Serial.print(humidity);
  Serial.println(" %");
  Serial.print(F("Temperature: "));
  Serial.print(temperature);
  Serial.println(F("°C "));
  Serial.print("Air Quality: ");
  Serial.print(airquality);
  Serial.println(" ppm");
  Serial.print("Vibration: ");
  Serial.print(vibrationfinal);
  Serial.println(" %");
  Serial.print("Air Pressure: ");
  Serial.print(pressure);
  Serial.println(" %");
  Serial.println();

  //Printing in OLED Display
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setCursor(0, 10);
  display.print("Humidity: ");
  display.print(humidity);
  display.println(" %");
  display.print("Temperature: ");
  display.print(temperature);
  display.println(" C");
  display.print("Air Quality: ");
  display.print(airquality);
  display.println(" ppm");
  display.print("Vibration: ");
  display.print(vibrationfinal);
  display.println(" %");
  display.print("Air Pressure: ");
  display.print(pressure);
  display.println(" %");
  display.display();

  //Uploading data to cloud
  if (!AirQuality.publish(airquality)) {
    Serial.println(F("Air Quality data failed to update"));
  }
  else {
    Serial.println(F("Air Quality data updated!"));
  }
  if (!AirPressure.publish(pressure)) {
    Serial.println(F("Air Pressure data failed to update"));
  }
  else {
    Serial.println(F("Air Pressure data updated!"));
  }
  if (!Humidity.publish(humidity)) {
    Serial.println(F("Humidity data failed to update"));
  }
  else {
    Serial.println(F("Humidity data updated!"));
  }
  if (!Temperature.publish(temperature)) {
    Serial.println(F("Temperature data failed to update"));
  } else {
    Serial.println(F("Temperature data updated!"));
  }
  if (!Vibration.publish(vibrationfinal)) {
    Serial.println(F("Vibration data failed to update"));
  }
  else {
    Serial.println(F("Vibration data updated!"));
  }

  //To append and send data to excel in case of anomaly
  String ds1, ds2, ds3, ds4, ds5, datas;
  bool dataAppended = false;  // Flag to track whether data was appended

  if (humidity > 90)
    ds1 = "humidity=" + String(humidity);
  else
    ds1 = "humidity=Normal";
  if (temperature > 35)
    ds2 = "temperature=" + String(temperature);
  else
    ds2 = "temperature=Normal";
  if (airquality > 600)
    ds3 = "airquality=" + String(airquality);
  else
    ds3 = "airquality=Normal";
  if (vibrationfinal > 85)
    ds4 = "vibration=" + String(vibrationfinal);
  else
    ds4 = "vibration=Normal";
  if (pressure > 20)
    ds5 = "airpressure=" + String(pressure);
  else
    ds5 = "airpressure=Normal";

  // Check if any data is appended
  if (ds1 != "humidity=Normal" || ds2 != "temperature=Normal" || ds3 != "airquality=Normal" || ds4 != "vibration=Normal" || ds5 != "airpressure=Normal") {
    datas = ds1+"&"+ds2+"&"+ds3+"&"+ds4+"&"+ds5;

    Serial.println();
    Serial.println(datas);
    sendData(datas);
  } else {
    Serial.println("No abnormal sensor data detected. No action taken.");
  }
  
  Serial.println();
  Serial.println();

